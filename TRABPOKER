typedef struct {
  t_carta cartas[5];
  t_valor_m valor;
} t_mao;

void troca(int *a, int *b){
  int aux;
  aux = *a;
  *a = *b;
  *b = aux;
}

void ordenaselection(int a, int v[5]){

  int x, y, min;
  for(x = 0; x < y - 1; x++) 
    min = x;
    for (y = x+1; y < n; y++)
      if (v[y] < v[min])
        min = y;
  troca(&v[x], &v[min]);
  }
}



