#include <stdio.h>
#define MAX 100

void min_max(int n, int v[MAX], int *max, int*min){
    max = v[i];
    min = v[i];
    for(int i = 0; i < n; i++){
        if(v[i] < min){
            min = v[i];
        }
        else if(v[i] > max){
            max = v[i];
        }
    }
}

void dois_maiores(int n, int v[MAX], int *pr, int *seg){
    pr = v[i];
    seg = v[i];
    for(int i = 0; i < n; i++){
        if(v[i] > pr){
            seg = pr;
            pr = v[i];
        }
        else if(v[i] > seg){
            max = v[i];

}

int main(){
    int n;
    int v[MAX];
    scanf("%d", &n);
    for(int i = 0; i < MAX; i++){
        scanf("%d", &v[i]);
    }
    int max, min;
    min_max(n, v[MAX], &max, &min);

    printf("%d", min);
    printf("%d", max);

    return 0;

}


